def solution(d, budget):
    d.sort()
    #1. 총합과 예산이 동일한 경우
    if sum(d) == budget:
        return len(d)
    #2-1. 총합이 예산보다 큰 경우
    else:
        # 2-2. 
        while sum(d)-budget > 0:
            if sum(d) - budget in d:
                d.remove(sum(d)-budget)
                return len(d)
            # 2-3.
            else:
                del d[-1]
    return len(d)